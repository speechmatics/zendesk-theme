                                        {{#each articles}}
                                            {{#if promoted}}
                                                <a href="{{ url }}" class="blocks-item-link">
                                            {{/if}}
                                        {{/each}}
                                            <span class="blocks-item-title"> {{ name }} </span>
                                            <span class="blocks-item-description">
                                                {{#each articles}}
                                                    {{#if promoted}}
                                                        {{ title }}
                                                    {{/if}}
                                                {{/each}}
                                            </span>
                                        </a>
                                    </li>
                                </ul>
                            </section>
                        {{/is}}
                    {{/each}}
                {{/each}}
            {{/each}}
            {{ pagination}}
        </ul>

